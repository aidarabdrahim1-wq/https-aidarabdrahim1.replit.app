# Environment for UBT AI Teacher backend
UBT_ENVIRONMENT=development

# AI provider: fallback | openai | anthropic | google
UBT_AI_PROVIDER=fallback

# Provider keys (optional if fallback)
UBT_OPENAI_API_KEY=
UBT_ANTHROPIC_API_KEY=
UBT_GOOGLE_API_KEY=

# CORS origins: comma-separated, default *
UBT_CORS_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
